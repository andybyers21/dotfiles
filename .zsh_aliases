alias v="nvim"
alias vv="nvim ."
alias :q="exit"

# Shortcut to init.vim from anywhere
nvi() {
    cd /Users/andybyers/.config/nvim
    nvim init.vim
    cd -
}

# Daily note push
notes push() {
    cd /Users/andybyers/Documents/Nota/2021_Notes
    git ac "$(date +"%F, %T")"
    git push
    cd -
}

